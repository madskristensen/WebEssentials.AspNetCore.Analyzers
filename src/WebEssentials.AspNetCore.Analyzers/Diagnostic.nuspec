<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>WebEssentials.AspNetCore.Analyzers</id>
    <version>1.0.0.0</version>
    <title>WebEssentials.AspNetCore.Analyzers</title>
    <authors>Mads Kristensen</authors>
    <owners>Mads Kristensen</owners>
    <licenseUrl>https://github.com/ligershark/WebEssentials.AspNetCore.Analyzers/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/ligershark/WebEssentials.AspNetCore.Analyzers</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/madskristensen/WebEssentials.AspNetCore.Analyzers/master/art/logo.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A collection of analyzers for ASP.NET Core</description>
    <copyright>Copyright Mads Kristensen</copyright>
    <tags>asp.net, core, roslyn, analyzers</tags>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>